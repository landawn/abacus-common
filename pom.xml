	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.8</source>
					<target>1.8</target>
				</configuration>
			</plugin>

			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-source-plugin</artifactId>
				<configuration>
					<excludes>
						<exclude>com/landawn/abacus/parser/**</exclude>
						<exclude>com/landawn/abacus/type/**</exclude>
					</excludes>
				</configuration>

				<executions>
					<execution>
						<id>attach-sources</id>
						<goals>
							<goal>jar</goal>
						</goals>
					</execution>
				</executions>
			</plugin>

			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-javadoc-plugin</artifactId>
				<configuration>
					<doclint>none</doclint>
					<sourceFileExcludes>
						<exclude>/com/landawn/abacus/core/**/*.java</exclude>
						<exclude>/com/landawn/abacus/dataSource/**/*.java</exclude>
						<exclude>/com/landawn/abacus/lock/**/*.java</exclude>
						<exclude>/com/landawn/abacus/type/**/*.java</exclude>
						<exclude>/com/landawn/abacus/logging/**/*.java</exclude>
					</sourceFileExcludes>
					<!--
					<excludePackageNames>com.landawn.abacus.core:com.landawn.abacus.dataSource:com.landawn.abacus.lock:com.landawn.abacus.type:com.landawn.abacus.logging</excludePackageNames>
					-->
				</configuration>

				<executions>
					<execution>
						<id>attach-javadocs</id>
						<goals>
							<goal>jar</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
		</plugins>
